{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "types": [
      "unplugin-icons/types/astro"
    ],
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@lib/*": [
        "src/lib/*"
      ],
      "@consts": [
        "src/consts.ts"
      ],
      "@types": [
        "src/types.ts"
      ],
      "@content": [
        "src/content/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}